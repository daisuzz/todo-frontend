{
  "name": "todo-front",
  "version": "1.0.0",
  "description": "My amazing Nuxt.js project",
  "author": "Daisaku Suzuki",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.ts --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.ts",
    "generate": "nuxt generate",
    "test": "jest",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore ."
  },
  "dependencies": {
    "consola": "latest",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "nuxt": "^3.4.3",
    "vuetify": "latest"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^0.5.6",
    "@nuxtjs/vuetify": "^1.0.0",
    "@types/jest": "^25.1.1",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^6.1.2",
    "husky": "^4.2.1",
    "jest": "^24.1.0",
    "lint-staged": "^10.0.7",
    "nodemon": "^1.18.9",
    "prettier": "^1.19.1",
    "vue-jest": "^4.0.0-0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*/**/*.{js,vue}": "npm run lint"
  }
}
